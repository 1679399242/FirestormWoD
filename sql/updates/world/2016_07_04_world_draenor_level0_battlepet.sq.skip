UPDATE creature_template SET IconName="", npcflag = npcflag & ~1073741824, creature_template.type=8, movementType=1, flags_extra=2, creature_template.exp=0 WHERE entry IN (SELECT CritterEntry FROM temp_battlepet_spawn_relation);

-- Draenor
UPDATE creature a SET a.id=(SELECT b.CritterEntry FROM temp_battlepet_spawn_relation b WHERE b.BattlePetEntry = a.id) WHERE a.id IN (SELECT BattlePetEntry FROM temp_battlepet_spawn_relation) AND map > 1000;
	

